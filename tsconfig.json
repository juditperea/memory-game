{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["dom", "es2016"], 
    "module": "commonjs",
    "esModuleInterop": true,
    "allowJs": true,
    "jsx": "react",
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "strictNullChecks": false
  },
  "include": ["src"]
}
